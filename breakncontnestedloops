#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;

    for (int x = 0; x <= sqrt(n); x++) {
        for (int y = 0; y <= sqrt(n); y++) {
            if (x * x + y * y == n) {
                cout << "(" << x << ", " << y << ")" << endl;
            }
        }
    }

    return 0;
}
